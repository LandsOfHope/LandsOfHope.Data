{
    "$id": "https://data.landsofhope.com/schemas/v1/accounts/returning/returning-character.json",
    "title": "ReturningCharacter",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "$type": {
            "const": "/v1/accounts/returning/returning-character"
        },
        "name": {
            "type": "string"
        },
        "race": {
            "$ref": "/schemas/v1/races-id.json"
        },
        "profession": {
            "$ref": "/schemas/v1/professions-id.json"
        },
        "sex": {
            "$ref": "/schemas/v0/characters/character-sex.json"
        },
        "currentImage": {
            "$ref": "/schemas/v1/characters/images-id.json"
        },
        "importableImages": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "$ref": "/schemas/v1/characters/images-id.json"
            }
        }
    },
    "required": [
        "$type",
        "name",
        "race",
        "profession",
        "sex",
        "currentImage",
        "importableImages"
    ]
}