{
    "$id": "https://data.landsofhope.com/schemas/v1/items/enhancements/item-patch.json",
    "title": "ItemPatch",
    "type": "object",
    "properties": {
        "$type": {
            "const": "/v1/items/enhancements/item-patch"
        },
        "id": {
            "$ref": "/schemas/v1/items-enhancements-patches-id.gen.json"
        },
        "effects": {
            "type": "object",
            "properties": {
                "skills": {
                    "type": "object",
                    "patternProperties": {
                        ".*": {
                            "type": "number"
                        }
                    },
                    "propertyNames": {
                        "$ref": "/schemas/v1/skills-id.gen.json"
                    },
                    "minProperties": 1,
                    "additionalProperties": false
                },
                "properties": {
                    "type": "object",
                    "patternProperties": {
                        ".*": {
                            "type": "number",
                            "minimum": 0,
                            "maximum": 1,
                            "multipleOf": 0.01
                        }
                    },
                    "propertyNames": {
                        "$ref": "/schemas/v1/characters/character-property-name.json"
                    },
                    "minProperties": 1,
                    "additionalProperties": false
                }
            },
            "additionalProperties": false,
            "anyOf": [
                {
                    "required": [
                        "skills"
                    ]
                },
                {
                    "required": [
                        "properties"
                    ]
                }
            ]
        }
    },
    "additionalProperties": false,
    "required": [
        "$type",
        "id",
        "effects"
    ]
}