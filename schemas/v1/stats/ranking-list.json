                            "name": {
                                "$ref": "/schemas/v1/characters/character-name.json"
                            },
                            "level": {
                                "type": "number"
                            },
                            "pictureID": {
                                "$ref": "/schemas/v1/characters/images-id.json"
                            }
                        }
                    }
                }
            }
        },
        {
            "properties": {
                "stat": {
                    "const": "strength"
                },
                "data": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "$ref": "/schemas/v1/characters/character-name.json"
                            },
                            "strength": {
                                "type": "integer"
                            },
                            "pictureID": {
                                "$ref": "/schemas/v1/characters/images-id.json"
                            }
                        }
                    }
                }
            }
        },
        {
            "properties": {
                "stat": {
                    "const": "intelligence"
                },
                "data": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "$ref": "/schemas/v1/characters/character-name.json"
                            },
                            "intelligence": {
                                "type": "integer"
                            },
                            "pictureID": {
